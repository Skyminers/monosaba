{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "manosaba",
  "version": "0.1.0",
  "identifier": "com.skyminer.manosaba",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "manosaba",
        "width": 1400,
        "height": 900
      }
    ],
    "security": {
      "csp": null,
      "assetProtocol": {
        "enable": true,
        "scope": ["**"]
      }
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "resources": ["config"],
    "macOS": {
      "minimumSystemVersion": "10.13"
    }
  }
}
